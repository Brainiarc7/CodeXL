      <Parent guid="guidCodeXLVSPackageCmdSet" id="cmdidCodeXLDebuggingActionsGroup"/>
    </CommandPlacement>

    <CommandPlacement guid="guidVSStd97" id="cmdidToggleBreakpoint" priority="0x0100">
      <Parent guid="guidCodeXLVSPackageCmdSet" id="cmdidCodeXLDebuggingBreakpointsMenuGroup"/>
    </CommandPlacement>
    <CommandPlacement guid="guidVSStd97" id="cmdidClearBreakpoints" priority="0x0300">
      <Parent guid="guidCodeXLVSPackageCmdSet" id="cmdidCodeXLDebuggingBreakpointsMenuGroup"/>
